<!-- Client new Section -->
<section class="lateral-separator top-separator" id="contact">
  <div class="container">
    <h2 class="text-center">Settings</h2>
    <hr class="star-primary">
    <div class="row">
      <div class="col-lg-12 mx-auto">
        <% flash.each do |key, value| %>
          <div class="<%= flash_class(key) %>">
            <%= value %>
          </div>
        <% end %>
        <%= render 'form', settings: @settings %>
      </div>
    </div>
  </div>
</section>
